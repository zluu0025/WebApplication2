@model IEnumerable<WebApplication2.Models.OrderLineSet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BookingSet.Contact_PhonbeNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmStartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmEndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReturnStatus)
            </th>
            <th></th>
        </tr>
    
    </thead>

<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BookingSet.Contact_PhonbeNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConfirmStartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConfirmEndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReturnStatus)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</tbody>
</table>


@section Scripts {
    @Scripts.Render("~/Scripts/DataTables/jquery.dataTables.js")
    @Scripts.Render("~/Scripts/DataTables/dataTables.bootstrap.js")
    <script>
$(document).ready(function () {
$('.table').DataTable();
});
    </script>
}
<link href="~/Content/DataTables/css/dataTables.bootstrap.min.css"
      rel="stylesheet" />